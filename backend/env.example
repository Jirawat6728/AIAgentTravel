# ============================================
# AI Travel Agent - Environment Variables
# ============================================
# Copy this file to .env and fill in your values
# cp env.example .env

# ============================================
# LLM Configuration (Gemini)
# ============================================
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL_NAME=gemini-1.5-flash
GEMINI_SONNET_MODEL=gemini-1.5-pro
GEMINI_TIMEOUT_SECONDS=60
GEMINI_MAX_RETRIES=3
ENABLE_GEMINI=true

# ============================================
# Database Configuration
# ============================================
# For Docker Compose, use service names:
# MONGODB_URI=mongodb://mongodb:27017
# REDIS_HOST=redis
# For local development:
MONGODB_URI=mongodb://localhost:27017
MONGODB_DATABASE=travel_agent

REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
REDIS_TTL=3600

# ============================================
# Google Services Configuration
# ============================================
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# ============================================
# Amadeus API Configuration
# ============================================
AMADEUS_API_KEY=your_amadeus_api_key_here
AMADEUS_API_SECRET=your_amadeus_api_secret_here
AMADEUS_ENV=test
AMADEUS_SEARCH_ENV=test
AMADEUS_BOOKING_ENV=test

# ============================================
# Authentication Configuration
# ============================================
SECRET_KEY=super-secret-key-for-travel-agent-123
SESSION_COOKIE_NAME=session_id
SESSION_EXPIRY_DAYS=30

# ============================================
# Payment Gateway (Omise)
# ============================================
OMISE_SECRET_KEY=your_omise_secret_key_here
OMISE_PUBLIC_KEY=your_omise_public_key_here
FRONTEND_URL=http://localhost:5173

# ============================================
# Logging Configuration
# ============================================
LOG_LEVEL=INFO
LOG_FILE=

# ============================================
# Agent Configuration
# ============================================
CONTROLLER_MAX_ITERATIONS=3
CONTROLLER_TEMPERATURE=0.3
RESPONDER_TEMPERATURE=0.7

# ============================================
# Admin Dashboard Configuration
# ============================================
ADMIN_PASSWORD=your_admin_password_here
ADMIN_ENABLED=true
ADMIN_REQUIRE_AUTH=true
